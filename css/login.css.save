@font-face {
    font-family: 'High Pixel 7';
    src: url(../fonts/high_pixel-7.ttf) format('truetype');
}

body {
    font-family: 'High Pixel 7';
    overflow: hidden;
    background: url('../images/back1.gif'); background-position: center;  background-repeat:no-repeat; background-size:cover;
}

.form{
    display: none;
    color: white;   
}
/* preloader */
#preloader {
    position: fixed;
    top:0; left:0;
    right:0; bottom:0;
    background: #000;
    z-index: 100;
}
#loader {
    width: 100px;
    height: 100px;
    position: absolute;
    left:50%; top:50%;
    background: url(../images/loader.gif) no-repeat center 0;
    margin:-50px 0 0 -50px;
}
.option{
    //border: solid black 3px;
    color: white;
}

.selected{
    background: url(../imageso
/select.png) no-repeat center 0;
    background-position: right;
}

.start{
    position: absolute;
    top: 80%;
}
